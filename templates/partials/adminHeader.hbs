<script>
    function openMenu() {
        $('#layout-menu').toggleClass('active');
    }
    $(document).ready(function () {
        // .scroll() creates an event when the user scrolls
        $(window).scroll(function () {
            // .scrollTop() retrieves vertical position of the scroll bar for the first element in a set of matched elements
            var scroll = $(window).scrollTop();
            if (scroll > 20) {
                $('.search-form-fixed').addClass('displayNav');
                $('.search-form-fixed-backdiv').removeClass('hide');
            } else {
                $('.search-form-fixed').removeClass('displayNav');
                $('.search-form-fixed-backdiv').addClass('hide');
            }
        });
    });

    //===========================================================================================
    // SHORTCUTS
    const searchInput = $('#search-form input')
    searchInput.on('focus', (e) => {
        keyboardJS.bind('esc', (e) => {
            event.preventDefault();
            searchInput[0].blur()
        });
    })
    // focus on search box
    keyboardJS.bind('ctrl + /', (e) => {
        event.preventDefault();
        searchInput[0].focus()
    });

    // open shortcut container
    keyboardShortcut = $('.keyboard-shortcuts-container')
    keyboardJS.bind('shift + /', (e) => {
        event.preventDefault();
        $('html').css('overflow-y', 'hidden')
        keyboardShortcut.addClass('active')
    });
    // close shortcut container
    keyboardJS.bind('esc', (e) => {
        if (keyboardShortcut.hasClass('active')) {
            $('html').css('overflow-y', 'auto')
            keyboardShortcut.removeClass('active')
        }
    });


    //page shortcuts
    //============================================
    // machines sales data
    keyboardJS.bind('alt + m', (e) => {
        e.preventDefault()
        window.location.replace("/vitco-impex/control/sales-report/machine/all")
    });
    // parts sales data
    keyboardJS.bind('alt + p', (e) => {
        e.preventDefault()
        window.location.replace("/vitco-impex/control/sales-report/parts/all")
    });
    // service report
    keyboardJS.bind('alt + s', (e) => {
        e.preventDefault()
        window.location.replace("/vitco-india/control/service-reports/all")
    });
    // open inbox
    keyboardJS.bind('alt + i', (e) => {
        e.preventDefault()
        window.location.replace("/vitco-india/control/admin/notifications")
    });
    // open complaints
    keyboardJS.bind('alt + c', (e) => {
        e.preventDefault()
        window.location.replace("/vitco-india/control/complaints")
    });
    // open profile
    keyboardJS.bind('alt + h', (e) => {
        e.preventDefault()
        window.location.replace("/vitco-india/control")
    });
    //================================================================================



    $(window).resize(function () { mediaQueriesJS() });
    function mediaQueriesJS() {
        pageWidth = $(window).width();
        if (pageWidth < 768) {
            searchInput.attr('placeholder', 'Search here...')
        } else {
            searchInput.attr('placeholder', 'press " Ctrl + / " to start searching')
        }
    }
    $('#search-form').on('submit', (e) => {
        e.preventDefault()
    })
</script>
