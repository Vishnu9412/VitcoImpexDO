{{#> style}}{{/style}}
<style>
    @import url('https://fonts.googleapis.com/css2?family=Open+Sans:wght@500&family=Poppins:ital,wght@0,400;0,500;0,600;1,500&display=swap');
    @import url('https://fonts.googleapis.com/css2?family=Open+Sans&display=swap');

    :root {
        --base-color: #26abe2;
    }

    .border-radius-right {
        border-radius: 0 5px 5px 0;
    }

    .opacity-3 {
        opacity: 0.3;
    }

    .base-color-text {
        color: #26abe2 !important;
    }

    .bg-base-color {
        background-color: #26abe2 !important;
    }

    .bg-text-base-color {
        background-color: #26abe2 !important;
        color: #fff !important;
    }

    .bg-text-base-color:hover {
        background-color: #2699ca !important;
        color: #fff !important;
    }

    .bg-text-base-color:focus {
        background-color: #2699ca !important;
        color: #fff !important;
    }

    body {
        font-family: 'Poppins', sans-serif;
    }

    .admin-notification-icon {
        color: #278ad6;
    }

    .admin-notification-icon:hover {
        color: #0961a5;
    }

    .admin-profile-dropdown {
        background-color: #fff;
        position: fixed;
        width: fit-content;
        top: 5em;
        right: 0em;
        padding: 1em 0;
        list-style: none;
        border-radius: 5px;
    }

    .custom-scrollbar::-webkit-scrollbar {
        width: 5px;
    }

    /* Track */
    .custom-scrollbar::-webkit-scrollbar-track {
        box-shadow: inset 0 0 5px grey;
        border-radius: 0;
    }

    /* Handle */
    .custom-scrollbar::-webkit-scrollbar-thumb {
        background: #26abe2;
        border-radius: 10px;
    }

    /* Handle on hover */
    .custom-scrollbar::-webkit-scrollbar-thumb:hover {
        background: #198ec1;
    }

    .input-icon-left {
        position: absolute;
        left: 5px;
        top: 7px;
    }

    #layout-menu {
        transition: all 0.3s ease-in-out;
    }

    #layout-menu.active {
        left: 16em !important;
    }

    @media (min-width: 1200px) {
        .layout-page {
            margin-left: 17em;
        }

        .layout-menu {
            position: fixed !important;
            top: 0;
            bottom: 0;
            left: 0;
            margin-right: 0 !important;
            margin-left: 0 !important;
        }
    }

    .fixed-bottom-right {
        position: fixed;
        bottom: 1em;
        right: 1em;
        z-index: 99;
    }

    .search-form-fixed-backdiv {
        height: 5em;
        width: 100%;
    }

    .search-form-fixed.displayNav {
        position: fixed;
        top: 0em;
        width: 76.3%;
        left: 18.7em;
    }

    .search-form-fixed .search-form input {
        background-color: transparent;
    }

    .keyboard-shortcuts-container {
        transition: all 0.2s ease-in-out;
        visibility: hidden;
        opacity: 0;
        color: #fff;
        height: 95vh;
        width: 97vw;
        position: fixed;
        top: 1em;
        left: 1em;
        background-color: #202120;
        z-index: 999999;
        font-family: 'Open Sans', sans-serif;
        border-radius: 5px;

    }

    .keyboard-shortcuts-container.active {
        visibility: visible;
        opacity: 1;

    }

    .keyboard-shortcuts-container ul li {
        background-color: transparent;
        border-color: #4e4e4e;
        color: #9f9e9f;
        padding: 0.5em;
        font-size: small;
        display: flex;
        justify-content: space-between;
    }

    .keyboard-shortcuts-container ul li span {
        margin: 0 1em;
    }

    .keyboard-shortcuts-container ul li span:nth-child(2) {
        color: white;
    }

    @media (max-width: 1200px) {
        .search-form-fixed.displayNav {
            left: 1em;
            top: 0em;
        }
    }

    .details-container {
        height: 100vh;
        width: calc(100vw - 16.5em);
        background-color: #fff;
        position: fixed;
        top: 0;
        right: 0;
        z-index: 999;
        padding: 2em;
        overflow-x: hidden;
    }

    @media (max-width: 1200px) {
        .details-container {
            width: 100vw;
        }
    }

    /*@media (max-width: 600px) {
        .details-container{
            padding: 1em 0;
            top: 0;
            
        }
    }
     @media (max-width: 368px) {
        .details-container{
            top: -11.11em;
        }
    }*/
</style>

<body>

    <div class="keyboard-shortcuts-container">
        <div class="container-fluid">
            <div class="row">
                <div class="col-12 mt-3">
                    <h3 style="font-weight: 400;">Keyboard shortcuts</h3>
                </div>
                <div class="col-md-5 mt-4 ms-3">
                    <h6>Page shortcuts</h6>
                    <ul class="list-group list-group-flush">
                        <li class="list-group-item"><span>Alt + M</span> <span>All machine sales data</span></li>
                        <li class="list-group-item"><span>Alt + P</span> <span>All parts sales data</span></li>
                        <li class="list-group-item"><span>Alt + S</span> <span>Open service reports</span></li>
                        <li class="list-group-item"><span>Alt + I</span> <span>Open inbox</span></li>
                        <li class="list-group-item"><span>Alt + C</span> <span>Open complaints</span></li>
                        <li class="list-group-item"><span>Alt + H</span> <span>Go to profile page</span></li>
                    </ul>
                </div>
                <div class="col-md-5 mt-4 ms-5 ps-5">
                    <h6>Common shortcuts</h6>
                    <ul class="list-group list-group-flush common-shortcuts">
                        <li class="list-group-item"><span>R</span> <span>Refresh all reports</span></li>
                        <li class="list-group-item"><span>Ctrl + /</span> <span>Start searching</span></li>
                        <li class="list-group-item"><span>Alt + A</span> <span>Close report details</span></li>
                        <li class="list-group-item"><span>Alt + =</span> <span>Add new report</span></li>
                        <li class="list-group-item"><span>Shift + /</span> <span>Open shortcut box</span></li>
                        <li class="list-group-item"><span>ESCAPE</span> <span>Close shortcut box</span></li>
                    </ul>
                </div>
            </div>
        </div>
        <div>
            <button class="border-0 text-primary"
                style="background-color:transparent;position:absolute;bottom:1em;right:1.5em;"
                onclick="keyboardShortcut.removeClass('active');$('html').css('overflow-y','auto')">CLOSE</button>
        </div>
    </div>

    <!-- Layout wrapper -->
