<div class="container-fluid mt-4">
  <div class="row">
    <div class="col-12 complaint-categories" style="overflow:auto;">
      <ul class="nav nav-pills mb-3 d-flex flex-nowrap" id="pills-tab" role="tablist">
        <li class="nav-item mx-2" role="presentation" onclick="getAllComplaints()" data-filter="all">
          <button class="nav-link px-2 nav-link-all active rounded-0 d-block top-header-1" id="pills-all-tab rounded-0"
            style="white-space: nowrap" data-bs-toggle="pill" data-bs-target="#pills-home" type="button" role="tab"
            aria-controls="pills-home" aria-selected="true" data-filter-value="All">
            All vouchers <span class="total-complaints"></span> </button>
        </li>
        <li class="nav-item mx-2" role="presentation" onclick="filterComplaint('Deleted')" data-filter="Deleted">
          <button class="nav-link px-2 nav-link-deleted rounded-0 top-header-accepted" id="pills-pending-tab rounded-0"
            style="white-space: nowrap" data-bs-toggle="pill" data-bs-target="#pills-deleted" type="button" role="tab"
            aria-controls="pills-deleted" aria-selected="false" data-filter-value="Pending">Accepted <span
              class="total-complaints"></span></button>
        </li>
        <li class="nav-item mx-2" role="presentation" onclick="filterComplaint('Deleted')" data-filter="Deleted">
          <button class="nav-link px-2 nav-link-deleted rounded-0 top-header-rejected" id="pills-pending-tab rounded-0"
            style="white-space: nowrap" data-bs-toggle="pill" data-bs-target="#pills-deleted" type="button" role="tab"
            aria-controls="pills-deleted" aria-selected="false" data-filter-value="Pending">Rejected <span
              class="total-complaints"></span></button>
        </li>
        <li class="nav-item mx-2" role="presentation" onclick="filterComplaint('Solved')" data-filter="Solved">
          <button class="nav-link px-2 nav-link-solved rounded-0 top-header-2" id="pills-solved-tab rounded-0"
            style="white-space: nowrap" data-bs-toggle="pill" data-bs-target="#pills-solved" type="button" role="tab"
            aria-controls="pills-solved" aria-selected="false" data-filter-value="Solved">Service
            <span class="total-complaints"></span></button>
        </li>
        <li class="nav-item mx-2" role="presentation" onclick="filterComplaint('Progress')" data-filter="Progress">
          <button class="nav-link px-2 nav-link-progress rounded-0 top-header-3" id="pills-progress-tab rounded-0"
            style="white-space: nowrap" data-bs-toggle="pill" data-bs-target="#pills-progress" type="button" role="tab"
            aria-controls="pills-progress" aria-selected="false" data-filter-value="Progress">Expense <span
              class="total-complaints"></span></button>
        </li>
        <li class="nav-item mx-2" role="presentation" onclick="filterComplaint('Pending')" data-filter="Pending">
          <button class="nav-link px-2 nav-link-pending rounded-0 top-header-4" id="pills-pending-tab rounded-0"
            style="white-space: nowrap" data-bs-toggle="pill" data-bs-target="#pills-pending" type="button" role="tab"
            aria-controls="pills-pending" aria-selected="false" data-filter-value="Pending">Delivery order <span
              class="total-complaints"></span></button>
        </li>

      </ul>
    </div>
